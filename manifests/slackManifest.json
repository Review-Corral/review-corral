{
  "display_information": {
    "name": "Review Corral",
    "description": "Github Pull Request Helper",
    "background_color": "#590088"
  },
  "features": {
    "bot_user": {
      "display_name": "Review Corral",
      "always_online": false
    },
    "slash_commands": [
      {
        "command": "/init",
        "url": "https://review-corral.vercel.app/api/slack/init",
        "description": "Initializes Review Corral in this channel",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "redirect_urls": [
      "https://reviewcorral.ngrok.io/api/slack/oauth",
      "https://review-corral.vercel.app/api/slack/oauth"
    ],
    "scopes": {
      "bot": [
        "chat:write",
        "commands",
        "incoming-webhook",
        "users:read",
        "chat:write.public"
      ]
    }
  },
  "settings": {
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://review-corral.vercel.app/api/slack/events"
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}
